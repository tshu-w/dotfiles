    <key>Label</key>
    <string>dev.friday.bot</string>

    <key>ProgramArguments</key>
    <array>
      <string>/bin/zsh</string>
      <string>-ilc</string>
      <string>/Users/wangtianshu/.config/pi-friday/startup.sh</string>
